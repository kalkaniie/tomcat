			</isNotNull>
			<isNotNull prepend="AND" property= "USERS_ID">
				USERS_ID = #USERS_ID#
			</isNotNull>
			<isNotEqual prepend="AND" property="USERS_DELEGATE" compareValue="">
				USERS_IDX IN (
